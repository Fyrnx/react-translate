{
    "manifest_version": 2,
    "name": "react",
    "version": "0.1",
  
    "description": "",

      "background": {
        "scripts": [
          "background.js"
        ]
      },

      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "css": ["/assets/index.css"]
        }
      ],
      

      "sidebar_action": {
        "default_title": "react",
        "default_panel": "index.html",
        "open_at_install": false,
        "default_icon": {
          "128":"./sources/nbl0h3i1.bmp"
        }
      },

      "browser_action": {
        "default_area": "personaltoolbar",
        "browser_style":true,
        "default_icon": {
          "128":"./sources/nbl0h3i1.bmp"
        }
      },
      
      "icons": { 
        "48":"./sources/nbl0h3i1.bmp",
        "128":"./sources/nbl0h3i1.bmp"
      },

      "permissions": ["tabs","<all_urls>","storage"]
  }
  